		},
		"updatedAt": 
		{
			"type": "datetime",
			"requerido": false
		},
		"active": 
		{
			"type": "boolean",
			"default": 1
		},
		"isSystem": 
		{
			"type": "boolean",
			"default": 0
		},
		"firstLogin": 
		{
			"type": "boolean",
			"default": 0
		},
		"changePassword": 
		{
			"type": "boolean",
			"default": 0
